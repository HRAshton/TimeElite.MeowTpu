<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <system.webServer>
        <security>
            <requestFiltering>
                <requestLimits maxQueryString="32768" />
            </requestFiltering>
        </security>
    </system.webServer>
  
    <system.web>
      <httpRuntime targetFramework="4.5" maxQueryStringLength="10240" enable="true" />
    </system.web>
</configuration>